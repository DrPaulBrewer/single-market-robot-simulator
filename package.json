{
  "name": "single-market-robot-simulator",
  "version": "5.1.3",
  "description": "simulations using market-contingent-example populated by various types of robots, such as Gode/Sunder ZI Robots, with configurable supply/demand",
  "bin": "build/index.js",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf ./build ./doc ; mkdir ./build",
    "build": "node_modules/.bin/babel build src/index.js -o build/index.js",
    "doc": "node_modules/.bin/esdoc -c esdoc.json",
    "lint": "node_modules/.bin/eslint src/index.js",
    "lint-test": "node_modules/.bin/eslint test/index.js",
    "test": "node_modules/.bin/mocha --require babel-core/register --reporter spec --slow 50 --timeout 300000",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports --compilers js:babel-register --timeout 300000",
    "go": "npm run clean && npm run lint && npm run lint-test && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drpaulbrewer/single-market-robot-simulator"
  },
  "keywords": [
    "robot-trading",
    "hft",
    "market-simulation",
    "financial-simulation"
  ],
  "author": "drpaulbrewer@eaftc.com",
  "license": "MIT",
  "dependencies": {
    "gini-ss": "^0.2.1",
    "market-agents": "^3.1.1",
    "market-example-contingent": "^1.0.2",
    "p-whilst": "^1.0.0",
    "positive-number-array": "^1.2.2",
    "simple-isomorphic-logger": "^2.1.0",
    "stats-lite": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.2",
    "esdoc": "^1.1.0",
    "eslint": "^5.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^6.0.0",
    "should": "^13.2.3"
  },
  "directories": {
    "test": "test"
  },
  "jsnext:main": "src/index.js"
}
